<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xjl.rxresult_x_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xjl.rxresult_x_demo.main.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.xjl.rxresult_x_demo.process.login.LoginActivity" />

        <activity android:name="com.xjl.rxresult_x_demo.process.auth.AuthActivity" />

        <activity android:name="com.xjl.rxresult_x_demo.process.register.RegisterActivity" />

        <activity android:name="com.xjl.rxresult_x_demo.process.pwdReset.PwdResetActivity" />

        <activity android:name="com.xjl.rxresult_x_demo.common.CommentActivity" />

    </application>

</manifest>